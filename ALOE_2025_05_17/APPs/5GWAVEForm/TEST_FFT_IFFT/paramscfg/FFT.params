
parameter {
	name = samplingfreqHz
	value = 48000.0
}
parameter {
	name = fftsize
	value = 512
}
# Number of FFT/IFFTs.
parameter {
	name = numffts
	value = 14
}
# Operation Mode: 1: FFT; 2: IFFT
parameter {
	name = op_mode
	value = 1
}

